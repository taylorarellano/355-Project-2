<html>
    <head>
        <title>Resumes</title>
    </head>
    <body>
        <h1>Resumes</h1>
        <a href="/resume/add">Add New Resume</a> <br />
        <br />

        <table border="1">
            <tr><th>User Name</th><th>Resume Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].first_name + ' ' + result[i].last_name %></a></td>
                <td><%= result[i].resume_name %></td>

                <td><form action="/resume/delete/">
                        <input type="hidden" name="resume_id" id="resume_id" value="<%= result[i].resume_id %>" />
                        <input type="submit" value="Delete"/>
                    </form>
                </td>
            </tr>
            <% } %>
        </table>
    </body>

</html>